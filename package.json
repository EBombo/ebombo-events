{
  "name": "ebombo-events",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev && cross-env NODE_ENV=development",
    "build": "next build && cross-env NODE_ENV=production",
    "start": "next start -p ${PORT:-5000}",
    "analyze": "cross-env ANALYZE=true next build",
    "deployDev": "npm run build && cross-env NODE_ENV=development firebase deploy --only functions,hosting -P dev",
    "deployFrontDev": "npm run build && cross-env NODE_ENV=development firebase deploy --only functions:next,hosting -P dev",
    "deployBackDev": "firebase deploy --only functions:api,hosting -P dev",
    "deployProd": "npm run build && cross-env NODE_ENV=production firebase deploy --only functions,hosting -P default",
    "deployFrontProd": "npm run build && cross-env NODE_ENV=production firebase deploy --only functions:next,hosting -P default",
    "deployBackProd": "firebase deploy --only functions:api,hosting -P default",
    "startBackEnd": "firebase emulators:start --only functions,auth,firestore --import=./dataEmulator --export-on-exit",
    "startFunctions": "firebase emulators:start --only functions",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "antd": "^4.14.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "country-list": "^2.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.1",
    "firebase": "^8.3.1",
    "firebase-admin": "^9.7.0",
    "firebase-functions": "^3.15.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^10.2.3",
    "next-offline": "^4.0.6",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.2",
    "nodemailer-smtp-transport": "^2.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-hook-form": "^4.6.2",
    "react-pwa-install-prompt": "^1.0.5",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "reactn": "^2.2.7",
    "resize-image": "^0.1.0",
    "styled-components": "^5.0.0",
    "url-assembler": "^2.1.1",
    "use-local-storage-state": "^9.0.2",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@next/bundle-analyzer": "^11.0.1",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.1",
    "cross-env": "^7.0.3",
    "prettier": "^2.2.1",
    "react-email-editor": "^1.3.0"
  },
  "engines": {
    "node": "10"
  }
}
