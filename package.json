{
  "name": "ebombo-events",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "copy:example": "cp .env.example ./.env",
    "dev": "npm run copy:example && next dev -p 3001",
    "reference": "next build && next export -> https://nextjs.org/docs/advanced-features/i18n-routing#how-does-this-work-with-static-generation",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start -p 5000",
    "format": "npx prettier --write .",
    "analyze": "cross-env ANALYZE=true next build",
    "deployBackDev": "firebase deploy --only functions -P dev",
    "deployBackProd": "firebase deploy --only functions -P default",
    "deployIndexesDev": "firebase deploy --only firestore:indexes -P dev",
    "deployIndexesProd": "firebase deploy --only firestore:indexes -P default",
    "deployHostingDev": "firebase deploy --only hosting -P dev",
    "deployHostingProd": "firebase deploy --only hosting -P default",
    "startFunctions": "firebase emulators:start --only functions",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "antd": "^4.14.1",
    "aos": "^2.3.4",
    "country-list": "^2.2.0",
    "ebombo-components": "github:EBombo/ebombo-components#0.0.3",
    "firebase": "^8.3.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "next": "^11.1.0",
    "next-offline": "^4.0.6",
    "react": "^17.0.2",
    "react-animations": "^1.0.0",
    "react-csv": "^2.0.3",
    "react-custom-roulette": "^1.1.8",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-ga": "^3.3.0",
    "react-hook-form": "^4.6.2",
    "react-pwa-install-prompt": "^1.0.5",
    "react-responsive": "^8.2.0",
    "react-router-dom": "^5.2.0",
    "reactn": "^2.2.7",
    "resize-image": "^0.1.0",
    "styled-components": "^5.0.0",
    "url-assembler": "^2.1.1",
    "use-local-storage-state": "^9.0.2",
    "xlsx": "^0.18.5",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@next/bundle-analyzer": "^11.0.1",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.12",
    "next-sitemap": "^1.6.203",
    "postcss": "^8.4.5",
    "prettier": "^2.2.1",
    "react-email-editor": "^1.3.0",
    "tailwindcss": "^3.0.5"
  },
  "prettier": {
    "printWidth": 120
  }
}
